# Our RDF graph locations
demo:graph_wikidata_properties_uri a cerif:ElectronicAddress ;
	cerif:has_URI <https://vre4eic.project.cwi.nl/gender/graph/wikidata_properties> .
demo:graph_hoyland_uri a cerif:ElectronicAddress ;
	cerif:has_URI <https://vre4eic.project.cwi.nl/gender/graph/hoyland> .
demo:graph_structure_uri a cerif:ElectronicAddress ;
	cerif:has_URI <https://vre4eic.project.cwi.nl/gender/graph/structure> .

demo:class_wikidata_properties_website a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.Output%20Types.Website%20content ;
        cerif:has_source          demo:graph_wikidata_properties_uri ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

demo:class_hoyland_website a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.Output%20Types.Website%20content ;
        cerif:has_source          demo:graph_hoyland_uri ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

demo:class_structure_website a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.Output%20Types.Website%20content ;
        cerif:has_source          demo:graph_structure_uri ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

demo:graph_wikidata_properties a cerif:Dataset ;
	rdfs:label "Events and structure graph"@en ;
	cerif:has_abstract "Talk of Europe: Description of the dates and order of the events in the plenary meetings, i.e. sessions, session days, agenda items, and speeches, and for the latter, the name of the speaker and spoken language. See https://doi.org/10.3233/SW-160227"@en ;
	cerif:has_name "EU Parliament debate structure without spoken texts"@en ;
	cerif:is_destination_of  demo:class_wikidata_properties_website ;
        cerif:is_source_of       demo:class1researchdata .

demo:graph_hoylang a cerif:Dataset ;
	rdfs:label "Members of Parliament background graph"@en ;
	cerif:has_abstract "Talk of Europe: Biographical and political information about the Members of Parliament, including first and last name, official MEP identification number, country of representation, birth date, and time- and role-annotated membership of EU parties and committees and national parties. Derived from Bjørn Høyland's database, see https://doi.org/10.1177/1465116508099764"@en ;
	cerif:has_name "Background info about MEPs from Høyland."@en ;
	cerif:is_destination_of  demo:class_hoyland_website ;
        cerif:is_source_of       demo:class2researchdata .

demo:graph_structure a cerif:Dataset ;
	rdfs:label "Wikidata aligment"@en ;
	cerif:has_abstract "Talk of Europe: Links between Members of Parliament and their corresponding resource in wikidata, using MEP ID to link. See http://doi.acm.org/10.1145/3201064.3201108"@en ;
	cerif:has_name "ToE MEP identifiers linked to wikidata ids."@en ;
	cerif:is_destination_of  demo:class_structure_website ;
        cerif:is_source_of       demo:class3researchdata .

demo:g1_rights_link a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.BREAD%20rights.edit ;
        cerif:has_source          demo:graph_wikidata_properties ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

demo:g2_rights_link a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.BREAD%20rights.edit ;
        cerif:has_source          demo:graph_hoylang ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

demo:g3_rights_link a cerif:FullLinkEntity ;
        cerif:has_classification  vre4eic:Classification.BREAD%20rights.edit ;
        cerif:has_source          demo:graph_structure ;
        cerif:has_startDate       "1900-01-01T00:00:00"^^xsd:dateTime ;
        cerif:has_endDate         "2099-12-31T23:59:59"^^xsd:dateTime .

