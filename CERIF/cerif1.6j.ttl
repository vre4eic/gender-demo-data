###  http://eurocris.org/ontology/cerif#has_amount
:has_amount rdf:type owl:ObjectProperty ;
            owl:inverseOf :is_amount_of ;
            rdfs:domain :LinkEntityWithAmount ;
            rdfs:range :Amount ;
            rdfs:comment ""@en ;
            rdfs:isDefinedBy <http://eurocris.org/ontology/cerif> ;
            rdfs:label "has amount"@en .


